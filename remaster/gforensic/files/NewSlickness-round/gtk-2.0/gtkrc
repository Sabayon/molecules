style "scrollbars" = "default"
{

engine "pixmap"
{

	image
	{
		function	= BOX
		recolorable	= TRUE
		detail		= "trough"
		file		= "/scrollbar/trough-scrollbar-horiz.png"
		border		= { 15, 15, 1, 1 }
		stretch		= TRUE
		orientation	= HORIZONTAL
	}

